{
    "title": "js笔记记录",
    "description": "记录 web端学习笔记",
    "author": "hyhello",
    "language": "zh-hans",
    "gitbook": "3.2.2",
    "root": "./docs",
    "output.name": "site",
    "links": {
        "sidebar": {
            "Home": "https://book.hyhello.top"
        }
    },
    "plugins": [
        "-lunr",
        "-search",
        "-sharing",
        "-highlight",
        "-livereload",
        "search-plus@^0.0.11",
        "simple-page-toc@^0.1.1",
        "github@^2.0.0",
        "github-buttons@2.1.0",
        "edit-link@^2.0.2",
        "prism@^2.1.0",
        "prism-themes@^0.0.2",
        "advanced-emoji@^0.2.1",
        "anchors@^0.7.1",
        "include-codeblock@^3.0.2",
        "ace@^0.3.2",
        "emphasize@^1.1.0",
        "katex@^1.1.3",
        "splitter@^0.0.8",
        "mermaid-gb3@2.1.0",
        "tbfed-pagefooter@^0.0.1",
        "expandable-chapters-small@^0.1.7",
        "sectionx@^3.1.0",
        "local-video@^1.0.1",
        "anchor-navigation-ex@0.1.8",
        "favicon@^0.0.2",
        "copy-code-button@^0.0.2",
        "todo@^0.1.3",
        "terminal@^0.3.2",
        "alerts@^0.2.0",
        "include-csv@^0.1.0",
        "puml@^1.0.1",
        "musicxml@^1.0.2",
        "klipse@^1.2.0",
        "versions-select@^0.1.1",
        "graph@^0.1.0",
        "chart@^0.2.0"
    ],

    "pluginsConfig": {
        "theme-default": {
            "showLevel": true
        },
        "prism": {
            "css": [
                "prism-themes/themes/prism-base16-ateliersulphurpool.light.css"
            ]
        },
        "github": {
            "url": "https://github.com/Hyhello/JS_Note"
        },
        "github-buttons": {
            "repo": "Hyhello/JS_Note",
            "types": [
                "star"
            ],
            "size": "small"
        },
        "include-codeblock": {
            "template": "ace",
            "unindent": true,
            "edit": true
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright © book.hyhello.top 2018",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "simple-page-toc": {
            "maxDepth": 3,
            "skipFirstH1": true
        },
        "edit-link": {
            "base": "https://github.com/Hyhello/JS_Note/edit/master",
            "label": "编辑当前页"
        },
        "anchor-navigation-ex": {
            "isRewritePageTitle": false,
            "tocLevel1Icon": "fa fa-hand-o-right",
            "tocLevel2Icon": "fa fa-hand-o-right",
            "tocLevel3Icon": "fa fa-hand-o-right"
        },
        "sectionx": {
            "tag": "b"
        },
        "favicon": {
            "shortcut": "./favicon.ico",
            "bookmark": "./favicon.ico"
        },
        "terminal": {
            "copyButtons": true,
            "fade": false,
            "style": "flat"
        },
        "versions": {
            "gitbookConfigURL": "https://book.hyhello.top/book.json",
            "options": [
                {
                    "value": "https://book.hyhello.top/canvas",
                    "text": "canvas动画",
                    "selected": true
                },
                {
                    "value": "https://book.hyhello.top/gulp",
                    "text": "gulp配置"
                },
                {
                    "value": "https://blog.hyhello.top",
                    "text": "博客"
                }
            ]
        }
    }
}
